{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Meteor": false,
    "Mongo": false,
    "Match": false,
    "Accounts": false,
    "Template": false,
    "Session": false,
    "Assets": false,
    "Email": false,
    "check": false,

    "Initiatives": true,
    "Notifications": true,
    "Users": true,

    "InitiativeCategories": true,
    "InitiativeCategorySlugs": true,
    "InitiativeSearch": true,
    "SearchSource": false,
    "ServiceConfiguration": false,
    "SSR": false,

    "checkEmailIsValid": true,
    "checkPasswordIsValid": true,
    "checkInitiative": true,
    "addOrRemoveVote": true,
    "followUnfollow": true,
    "configureFacebook": true,
    "sendWelcomeEmail": true,
    "smtp": true,
    "goBack": true,

    "SyncedCron": false,

    "Throttle": false,
    "Router": false,

    "$": false,
    "_": false,
    "sAlert": false,
    "s": false,
    "moment": false,
    "faker": false
  },
  "rules": {
    "strict": [2, "never"],
    "eol-last": 0,
    "comma-dangle": [2, "never"],
    "no-constant-condition": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-reserved-keys": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": ""}],
    "dot-location": [2, "property"],
    "eqeqeq": 1,
    "no-else-return": 1,
    "no-eval": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-spaces": [2, { exceptions: { "Property": false } }],
    "no-native-reassign": [2, {"exceptions": []}],
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-redeclare": 1,
    "no-script-url": 1,
    "no-unused-expressions": 1,
    "no-void": 1,
    "radix": 1,
    "vars-on-top": 1,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [2, {"properties": "always"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "consistent-this": [0, "self"],
    "func-names": 1,
    "func-style": [2, "expression"],
    "indent": [2, 2],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "max-nested-callbacks": [2, 3],
    "new-cap": [1, {newIsCap: true, capIsNew: false}],
    "new-parens": 1,
    "newline-after-var": [2, "always"],
    "no-array-constructor": 1,
    "no-inline-comments": 1,
    "no-lonely-if": 1,
    "no-multiple-empty-lines": [1, {max: 1}],
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "object-curly-spacing": [2, "never"],
    "one-var": [2, "never"],
    "operator-assignment": [2, "always"],
    "operator-linebreak": [2, "after"],
    "padded-blocks": [2, "never"],
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single", "avoid-escape"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "semi": [2, "always"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": [2, {"int32Hint": true}],
    "space-return-throw-case": 1,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "spaced-comment": [2, "always"],
    "wrap-regex": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-const": 0
  }
}